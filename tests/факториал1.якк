#приложить "стандарт.якк"

действо факториал_рекурсивный(цель эн)
{
	аще (эн == 1)
	{
		воздать 1;
	}

	воздать эн * факториал_рекурсивный(эн - 1);
}

действо факториал_итеративный(цель эн)
{
	цель результат = 1;

	ради (цель i = 1; i < эн + 1; i = i + 1)
	{
		результат = результат * i;
	}

	воздать результат;
}

действо царь_батюшка_главный()
{
	цель n = принять_нумер();

	явить_нумер(факториал_рекурсивный(n));
	явить_нумер(факториал_итеративный(n));


	воздать 0;
}
